@{
    List<string> uniqueNames = ViewBag.dep;
}

<h1>Dept Names</h1>
<ul>
    @foreach(string @item in uniqueNames)
    {
        <li><a asp-controller="Employee" asp-action="Dept" asp-route-id="@item">@item</a></li>
    }
</ul>